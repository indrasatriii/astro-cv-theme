---
import { skills } from '../data/skills.js';
---
<div class="skills-section">
  {skills.map((group) => (
    <section class="skill-group">
      <h2>{group.category}</h2>
      <ul>
        {group.items.map((item) => (
          <li>{item}</li>
        ))}
      </ul>
    </section>
  ))}
</div>
